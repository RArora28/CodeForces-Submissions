#include<iostream>

int main()
{
  int a, b, c, x, y, z , s = 0;

  cin>>a>>b>>c;
  cin>>x>>y>>z;

  if(a >= x)
    s += x + (a - x) / 2;
  else
    s += a;

  if(b >= y)
    s += y + (b - y) / 2;
  else
    s += b;

  if(c >= z)
    s += z + (c - z) / 2;
  else
    s += c;

  if(s >= (x + y + z))
    cout<<"YES"<<endl;
  else
    cout<<"NO"<<endl;

  return 0;
}
